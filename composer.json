{
  "name": "contentful/contentful-bundle",
  "description": "A Symfony2 Bundle to integrate the Contentful CDA SDK",
  "keywords": ["contentful", "cms", "sdk", "symfony2"],
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=5.5.9",
    "contentful/contentful": "dev-psr-6",
    "symfony/framework-bundle": "~2.7|~3.0",
    "symfony/http-kernel": "~2.7|~3.0",
    "symfony/console": "~2.7|~3.0",
    "symfony/http-foundation": "~2.7|~3.0",
    "symfony/dependency-injection": "~2.7|~3.0",
    "symfony/config": "~2.7|~3.0"
  },
  "require-dev": {
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "jakub-onderka/php-parallel-lint": "^0.9.2"
  },
  "autoload": {
    "psr-4": {
      "Contentful\\ContentfulBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.0-dev"
    }
  },
  "repositories": [
    { "type": "vcs", "url": "https://github.com/rdey/contentful.php" }
  ]
}
